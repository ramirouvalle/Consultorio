<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<AnchorPane id="pnlBg" fx:id="pnlBg" prefHeight="300.0" prefWidth="500.0" styleClass=".grid" stylesheets="@../resources/css/login.css" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <GridPane fx:id="gridPane" layoutX="20.0" layoutY="210.0" prefHeight="59.0" prefWidth="460.0" styleClass="grid" stylesheets="@../resources/css/login.css">
         <children>
            <TextField fx:id="txtUser" styleClass="txt" stylesheets="@../resources/css/login.css" GridPane.columnIndex="1" />
            <PasswordField fx:id="pwPassword" styleClass="txt" stylesheets="@../resources/css/login.css" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label stylesheets="@../resources/css/login.css" text="%lblUser" />
            <Label stylesheets="@../resources/css/login.css" text="%lblPassword" GridPane.rowIndex="1" />
            <Button fx:id="btnAccept" mnemonicParsing="false" onAction="#btnAccept_onClick" styleClass="btn" stylesheets="@../resources/css/login.css" text="%btnAccept" GridPane.columnIndex="2" />
            <Button fx:id="btnCancel" mnemonicParsing="false" onAction="#btnCancel_onClick" styleClass="btn" stylesheets="@../resources/css/login.css" text="%btnCancel" GridPane.columnIndex="2" GridPane.rowIndex="1" />
         </children>
         <columnConstraints>
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="103.0" minWidth="10.0" prefWidth="93.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="204.0" minWidth="10.0" prefWidth="193.0" />
            <ColumnConstraints hgrow="SOMETIMES" maxWidth="229.0" minWidth="10.0" prefWidth="164.0" />
         </columnConstraints>
         <rowConstraints>
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
            <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
         </rowConstraints>
      </GridPane>
      <Label layoutY="50.0" prefHeight="17.0" prefWidth="500.0" styleClass="title" stylesheets="@../resources/css/login.css" text="%lblTitle" />
   </children>
</AnchorPane>
